{
  "name": "electron-quick-start",
  "version": "1.0.0",
  "description": "A minimal Electron application",
  "main": "dist/main.js",
  "scripts": {
    "start": "node ./dist/main.js",
    "back:build": "npx tsc",
    "back:run": "npm run back:build && npm run start",
    "front:build": "npm --prefix ./front run build",
    "all:build": "npm run front:build && npm run back:build && npm run start",
    "all:run": "npm run all:build",
    "test": "jest --coverage ./back/test",
    "test:watch": "jest --watch ./back/test"
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [],
  "author": "GitHub",
  "license": "CC0-1.0",
  "type": "commonjs",
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.3",
    "@types/node": "^16.11.10",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "eslint": "^8.3.0",
    "eslint-config-google": "^0.14.0",
    "jest": "^27.3.1",
    "supertest": "^6.1.6",
    "ts-jest": "^27.0.7",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "@types/http-status-codes": "^1.2.0",
    "express": "^4.17.1",
    "http-status-codes": "^2.1.4"
  }
}
